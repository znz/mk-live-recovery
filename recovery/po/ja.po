#: restore.sh:18
#, sh-format
msgid "Format $DISK, OK?"
msgstr "$DISK をフォーマットします。よろしいですか?"

#: restore.sh:24
#, sh-format
msgid "Format $DISK, or shutdown now."
msgstr ""
"$DISK をフォーマットします。問題があればこのまま続行せずにシャットダウンして"
"ください。"

#: restore.sh:30
msgid "Partitioning..."
msgstr "パーティション設定中..."

#: restore.sh:33
#, sh-format
msgid "$DISK is currently in use. Can not format."
msgstr "$DISK が使用中です。フォーマットできません。"

#: restore.sh:38
#, sh-format
msgid "Formatting ${DISK}5 as swap..."
msgstr "${DISK}5をスワップ領域として初期化中..."

#: restore.sh:40
#, sh-format
msgid "Formatting ${DISK}1 as root partition..."
msgstr "${DISK}1をルート領域として初期化中..."

#: restore.sh:42
#, sh-format
msgid "Mounting ${DISK}1 to /target..."
msgstr "${DISK}1を/targetにマウント中..."

#: restore.sh:45
msgid "Copying files to target partition..."
msgstr "ターゲット領域にファイルをコピー中..."

#: restore.sh:47
msgid "Setup boot loader..."
msgstr "ブートローダ設定中..."

#: restore.sh:51
msgid "Unmounting target partition..."
msgstr "ターゲット領域をアンマウント中..."

#: restore.sh:55
msgid "done."
msgstr "完了。"

#: restore.sh:61
msgid "Recovery finished. Please reboot."
msgstr "リカバリが完了しました。再起動してください。"
